/* ~~~~~~~~~~ header adjustments ~~~~~~~~~~ */

.site-header { background-color: #000; border-bottom: 2px solid #0dd1d1;}
.site-header .navbar { padding: 0.5rem 0;}
.lefthead a { margin-top: 40px; }
.logo-holder a { margin: 0 auto; position: absolute; top: 42px; left: 0; right: 0; text-align: center; z-index: 9; }
.site-header ul.nav.social li a { color: #0dd1d1; font-size: 16px; }
.hdr-menu ul li a { color: #fff; text-transform: uppercase; font-size: 14px; font-weight: 700; font-family:"aktiv-grotesk-condensed"; }
.hdr-menu { position: relative; z-index: 99; }

/* =========== for Slider css =========== */

.main-wrapper { background: #0d0b0f; background: linear-gradient(0deg, #0d0b0f 20%, #122427 100%); padding: 30px 0 30px; position: relative; }
.buy-btn { /*position: absolute; right: 50px; top: 30px;*/padding-right: 1rem; position: relative; z-index: 99; }
.buy-btn a.btn { /*box-shadow: 0px 0px 24px 0px rgb(13 209 209 / 21%);*/ padding: 13px 30px; border-radius: 12px;}
.transfer-cntnt { padding: 25px 0 25px; }
.transfer-cntnt p { font-size: 18px; line-height: 1.4; }
.transfer-cntnt p b { font-weight: bold; }
.transfer-amountbox { border: 1px solid rgba(255,255,255,0.15); background: #060708; background: radial-gradient(circle, rgb(15 18 23) 0%, rgba(7,11,12,1) 100%); border-radius: 10px; padding: 25px; max-width: 850px; margin: 0 auto;}

/*.transfer-amountbox input[type="text"] { max-width: 240px; width: 45%; margin: 0 auto 25px; background-color: transparent; border: 0; border-bottom: 1px solid rgba(255,255,255,0.5); border-radius: 0; font-size: 16px; text-align: center; text-transform: uppercase; font-weight: 300; }  */

.transfer-amountbox input[type="number"]::-moz-placeholder{color:#fff;}
.transfer-amountbox input[type="number"]:-ms-input-placeholder{color:#fff;}
.transfer-amountbox input[type="number"]::-webkit-input-placeholder{color:#fff;}
.transfer-amountbox input[type="number"] { max-width: 240px; width: 45%; margin: 0 auto 50px; background-color: transparent; border: 0;  border-bottom: 1px solid rgba(255,255,255,0.15); border-radius: 0; font-size: 16px; text-align: center; text-transform: uppercase; font-weight: 300; color: #fff; }
.transfer-amountbox input[type="number"]:focus { outline: 0; box-shadow: inherit; }
.transfer-amountbox input[type="number"]::-moz-placeholder{color:#fff;}
.transfer-amountbox input[type="number"]:-ms-input-placeholder{color:#fff;}
.transfer-amountbox input[type="number"]::-webkit-input-placeholder{color:#fff;}
.transfer-amountbox .btn-holder { margin-left: -15px; margin-right: -15px; position: relative; margin-top: 25px; } 
.transfer-amountbox .btnbox { -ms-flex: 0 0 50%; flex: 0 0 50%; max-width: 50%; padding-left: 15px; padding-right: 15px; }
.btn-holder:after { content: ""; position: absolute; left: 0; right: 0; }
.transfer-amountbox .btnbox span { padding: 28px 20px; text-align: center; border: 1px solid rgba(255,255,255,0.15); font-size: 20px; color: #0dd1d1; font-weight: 300; text-transform: uppercase; display: block; background-color: #030203; border-radius: 4px; position: relative; }
.amount-percentage-box { position: relative; }
.amount-percentage-midle { width: 77px; height: 266px; border: 1px solid rgba(13, 211, 211, 0.45); margin: 0 auto; position: relative; }
.amount-percentage-circle-inr { width: 90px; height: 90px; line-height: 90px; background: #000; border:1px solid rgb(13, 211, 211, 0.45); border-radius: 50%; position: absolute; left: 0; right: 0; bottom: 0; margin: 0 auto; text-align: center; display: -ms-flexbox; display: flex; -ms-flex-wrap: wrap; flex-wrap: wrap; -ms-flex-align: center!important; align-items: center!important; z-index: 99; }
.amount-percentage-circle span { display: block; font-size: 12px; color: #0dd1d1; font-weight: 500; margin-top: 5px;}
.amount-percentage-circle h3 { margin: 0; width: 100%; font-weight: 300; letter-spacing: 1px; font-size: 22px;}
.amount-percentage-circle { position: absolute; left: 0; right: 0; bottom: 50%; height: 90px;transition: 1s cubic-bezier(0, 0, 0, 1.5) all;  /*transition: 1s linear all; transform: translateY(50%); */ z-index: 9; }

/*.amount-percentage-circle:after { content: ""; background-image: url(../images/puller.png);background-size: 100%; background-position: center center; background-repeat: no-repeat; width: 400px; height: 80px; position: absolute; left: 0; right: 0; top: 50%; transform: translateY(-50%); margin: 0 auto; text-align: center; }*/

.puller-box { width: 260px; height: 1px; position: absolute; left: 0; right: 0; top: 50%; -webkit-transform: translateY(-50%); -moz-transform: translateY(-50%); -ms-transform: translateY(-50%); -o-transform: translateY(-50%); transform: translateY(-50%); margin: 0 auto; text-align: center; background-color: rgba(13, 211, 211, 0.45);}
.puller-box:before {  content: ""; background-image: url(../images/flame_animate.gif);background-size: 100%; background-position: center center; background-repeat: no-repeat; width: 40px; height: 50px; position: absolute; left: -10px; top: 45%; -webkit-transform: translateY(-65%); -moz-transform: translateY(-65%); -ms-transform: translateY(-65%); -o-transform: translateY(-65%); transform: translateY(-65%); margin: 0 auto; text-align: center; }
.puller-box:after {  content: ""; background-image: url(../images/flame_animate.gif);background-size: 100%; background-position: center center; background-repeat: no-repeat; width: 40px; height: 50px; position: absolute; right: -10px; top: 45%; -webkit-transform: translateY(-65%); -moz-transform: translateY(-65%); -ms-transform: translateY(-65%); -o-transform: translateY(-65%); transform: translateY(-65%); margin: 0 auto; text-align: center; }

.percentage-height { position: absolute; left: 0; right: 0; bottom: 0; background: linear-gradient(-180deg, rgba(0,116,117,1) 0%, rgb(13 13 13) 84%); transition: 1s cubic-bezier(0, 0, 0, 1.5) all; /* transition: 1s linear all; */ }
/*.amount-percentage-midle:after { content: ""; position: absolute; left: 0; right: 0; margin: 0 auto; bottom: -61px; width: 1px; height: 61px; background-color: #5b5b5b; }*/
.transfer-amountbox .btn-holder:after { content: ""; position: absolute; left: 0; right: 0; margin: 0 auto; top: -25px; width: 1px; height: 67px; background-color: rgba(255,255,255,0.15);}
.transfer-amountbox .btnbox span:after { content: ""; position: absolute; right: -50px; width: 50px; height: 1px; background: rgba(255,255,255,0.15); top: 50%; -webkit-transform: translateY(-50%); -moz-transform: translateY(-50%); -ms-transform: translateY(-50%); -o-transform: translateY(-50%); transform: translateY(-50%); }
.transfer-amountbox .btnbox:nth-child(2) span:after { display: none; }




