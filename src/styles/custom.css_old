body { margin: 0; padding: 0; font-family:"aktiv-grotesk-condensed"; font-size: 14px; font-weight: 300; color: #282828; line-height: 1.4;}
h1, h2, h3, h4, h5, h6 { margin:0 0 20px; padding:0; line-height: 1.2; font-weight: 500; color: #282828; font-family:"aktiv-grotesk-condensed"; }
h1{ font-size:48px; }
h2{ font-size:36px; }
h3{ font-size:32px; }
h4{ font-size:24px;}
h5{ font-size:20px;}
h6{ font-size:16px; line-height: 1.2;}
.large-h2 { font-size: 48px;}
p{ margin:0 0 20px; padding:0; }
ul li {font-family:"aktiv-grotesk-condensed"; line-height: 1.6; font-weight: 400; position: relative; }
img{ max-width: 100%;}
a, img{ border:0; text-decoration: none; outline: none;}
a, a:link, a:visited, a:focus { /*color: #000; */outline: none; text-decoration: none; transition:all 0.3s ease 0s;}
a:hover{ text-decoration:none; color: #ffd800;}

input, textarea, select{ font-weight: 400; font-size: 15px; outline: none;}
/*input[type="submit"]{ font-weight: 400; font-size: 15px;  outline: none;}*/

/* --- for placeholder color --- */

.form-control::-moz-placeholder{color:#a4a4a3;}
.form-control:-ms-input-placeholder{color:#a4a4a3;}
.form-control::-webkit-input-placeholder{color:#a4a4a3;}

/* --- for placeholder color --- */


.link-arrow {font-size: 12px; text-transform: uppercase; font-weight: bold; position: relative;  margin-top: 35px; color: #000; display: block; margin-bottom: 15px;}
.link-arrow svg { width: 7px; position: relative; top: 1px;  margin-left: 30px; color: #D1AA69; letter-spacing: 1px;}
.t-white, .t-white p, .t-white h1, .t-white h2, .t-white h3, .t-white h4, .t-white h5, .t-white h6, .t-white a {color: #fff;}
.t-sky, .t-sky p, .t-sky h1, .t-sky h2, .t-sky h3, .t-sky h4, .t-sky h5, .t-sky h6, .t-sky a {color: #0dd1d1;}
.t-black, .t-black p, .t-black h1, .t-black h2, .t-black h3, .t-black h4, .t-black h5, .t-black h6, .t-black a {color: #333333;}
.t-bold {font-weight: 700;}
.t-blue, a.t-blue { color: #00d8d8;}
.rel-position {position: relative;}
h2.page-title {  text-transform: none; font-weight: bold; }
h3.sub-title{ font-weight: bold;  text-transform: none; }
.comn_bg {background-size: cover; background-repeat: no-repeat; background-position: center center;}

.bg-black { background-color: #000; }
.overlay-content { position: relative; z-index: 9;}



/* --- Button Styles --- */
a.btn, .btn { display: inline-block; vertical-align: middle; color: #fff; padding: 12px 30px; font-size: 14px; font-weight: 400; border: 1px solid #00d8d8; background: rgb(0,116,117); background: linear-gradient(86deg, #0e4d4f 0%, #0e4a4c 84%); position: relative; z-index: 1; line-height: normal; border-radius: 8px; font-family:"aktiv-grotesk-condensed"; text-transform: uppercase; transition:all 0.3s ease 0s; }
.btn.solid-color {  color: #333; background: #00d8d8;}
.btn.border { background:none; color: #00d8d8; border: 1px solid #00d8d8!important; }
.btn.border.white, a.btn.border.white{ background:none; color: #fff; border: 1px solid #fff!important; }
a.btn:hover, .btn:hover, .btn.border.white:hover{ color: #fff; box-shadow: 0px 0px 15px 0px #0dd1d157;}
.btn.rounded {  border-radius: 30px!important;}
.btn.medium-size { /* Your custom Styles */ }
.btn.large-size { /* Your custom Styles */ padding: 14px 65px; font-size: 18px; }
a.btn:focus{outline: none; box-shadow: none;}
.btn.focus, .btn:focus { outline: 0; box-shadow: none;}
.btn-wrapper{margin-top: 50px;}
.btn-wrapper.inline a.btn{display: inline-block; margin: 0 10px;}
.btn-wrapper.inline a.btn:first-child{margin-left: 0;}
a.btn.big { padding: 10px 60px; font-weight: 600; color: #141b1b;}
button:focus { outline: 0;}
.t-poppins { font-family: 'Poppins', sans-serif; }

