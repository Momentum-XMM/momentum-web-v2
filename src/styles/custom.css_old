.fancybox-is-open .fancybox-stage, .fancybox-slide { overflow: inherit !important;}
.fancybox-content{ width: 700px; padding: 0 40px; height: 100%; overflow-y: hidden !important; overflow-x: hidden; background-color: transparent; }
.fancybox-content iframe { width: 100%; height: 100%; border: 0; }
.fancybox-is-open .fancybox-bg { background-color: rgba(130,130,130);}
.fancybox-slide--html .fancybox-close-small { position: absolute; right: 0; opacity: 1; padding: 0;
 width: 24px; height: 24px; }
.fancybox-slide--html .fancybox-close-small:before { content: ""; width: 24px; height: 3px; display: block; background: #fff; margin-bottom: 5px; transition: all 0.3s; backface-visibility: hidden; transform: rotate(58deg) translate(4px, 5px); }
.fancybox-slide--html .fancybox-close-small:after { content: ""; width: 24px; height: 3px; display: block; background: #fff; margin-bottom: 5px; transition: all 0.3s; backface-visibility: hidden; transform: rotate(-58deg) translate(0px, -3px); }
.fancybox-button svg { display: none; }
.fancybox-is-open .fancybox-bg { background-color: #0a1011;}


